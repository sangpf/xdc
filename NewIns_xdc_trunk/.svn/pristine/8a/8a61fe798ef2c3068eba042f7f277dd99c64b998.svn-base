package com.newins.model;
/**
 * 广告实体类
 * @author Zhang
 *
 */
public class AdInfo {
	private int adId;//广告id
	private String adImg;//广告图片地址
	private String adLink;//广告连接
	private String adTitle;//广告标题
	public int getAdId() {
		return adId;
	}
	public void setAdId(int adId) {
		this.adId = adId;
	}
	public String getAdImg() {
		return adImg;
	}
	public void setAdImg(String adImg) {
		this.adImg = adImg;
	}
	public String getAdLink() {
		return adLink;
	}
	public void setAdLink(String adLink) {
		this.adLink = adLink;
	}
	public String getAdTitle() {
		return adTitle;
	}
	public void setAdTitle(String adTitle) {
		this.adTitle = adTitle;
	}
	/**
	 * 全参数构造
	 * @param adId
	 * @param adImg
	 * @param adLink
	 * @param adTitle
	 */
	public AdInfo(int adId, String adImg, String adLink, String adTitle) {
		super();
		this.adId = adId;
		this.adImg = adImg;
		this.adLink = adLink;
		this.adTitle = adTitle;
	}
	/**
	 * 空参构造
	 */
	public AdInfo() {
		super();
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "广告Id:"+adId+"--广告图片路径:"+adImg+"--广告链接:"+adLink+"--广告标题:"+adTitle;
	}
	
	
	
}
