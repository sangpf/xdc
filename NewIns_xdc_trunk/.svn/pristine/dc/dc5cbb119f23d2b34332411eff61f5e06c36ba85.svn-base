<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper .//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.newins.dao.SurveyDeliveryInfoMapper">
	<resultMap type="com.newins.model.SurveyDeliveryInfo" id="deliveryInfoMap">
	</resultMap>

	<select id="getDeliveryInfo" resultMap="deliveryInfoMap">
		SELECT
		ni_award.awardId,ni_award.awardName,lotteryId,awardType,ni_ad_info.adType,ni_ad_info.replaceAdId,awardLink,awardNum,ni_ad_info.adId,adImg,adLink,thumbUp,thumbDown,relatedStr1,relatedStr2,relatedStr3,relatedUrl1,relatedUrl2,relatedUrl3,resultMsg,ni_survey_delivery.sqnId
		as
		qnId,sqnCategory,ni_survey_questionnaire.keyQuestionNum,ni_survey_delivery.evaluateId,ni_survey_delivery.titleTag
		FROM
		ni_award,ni_award_pool,ni_ad_info,ni_survey_delivery,ni_survey_questionnaire
		WHERE ni_award.awardPoolId = ni_award_pool.awardPoolId
		AND ni_award.awardId = ni_survey_delivery.award1Id
		AND ni_survey_delivery.adId = ni_ad_info.adId
		AND
		ni_survey_delivery.deliveryId = #{deliveryId}
		AND
		ni_survey_delivery.sqnId=ni_survey_questionnaire.sqnId AND
		ni_survey_delivery.channelId=1
	</select>
</mapper>