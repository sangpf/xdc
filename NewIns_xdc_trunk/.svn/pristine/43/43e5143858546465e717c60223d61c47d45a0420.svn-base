package com.newins.model;

import java.sql.Timestamp;

/**
 * @Description 留言建议实体类
 * @author 星仔
 * @time 2017年2月23日上午10:30:36
 */
public class NiMessageboardModel {
	private int messageId;
	private int userId;
	private String messageContent;
	private Timestamp leaveMessageTime;
	private String comment;
	public int getMessageId() {
		return messageId;
	}
	public void setMessageId(int messageId) {
		this.messageId = messageId;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public String getMessageContent() {
		return messageContent;
	}
	public void setMessageContent(String messageContent) {
		this.messageContent = messageContent;
	}
	public Timestamp getLeaveMessageTime() {
		return leaveMessageTime;
	}
	public void setLeaveMessageTime(Timestamp leaveMessageTime) {
		this.leaveMessageTime = leaveMessageTime;
	}
	public String getcomment() {
		return comment;
	}
	public void setcomment(String comment) {
		this.comment = comment;
	}
	public NiMessageboardModel(int messageId, int userId,
			String messageContent, Timestamp leaveMessageTime, String comment) {
		super();
		this.messageId = messageId;
		this.userId = userId;
		this.messageContent = messageContent;
		this.leaveMessageTime = leaveMessageTime;
		this.comment = comment;
	}
	public NiMessageboardModel() {
		super();
	}
	@Override
	public String toString() {
		return "NiMessageboardModel [messageId=" + messageId + ", userId="
				+ userId + ", messageContent=" + messageContent
				+ ", leaveMessageTime=" + leaveMessageTime + ", comment="
				+ comment + "]";
	}
	public NiMessageboardModel(int userId, String messageContent,
			Timestamp leaveMessageTime, String comment) {
		super();
		this.userId = userId;
		this.messageContent = messageContent;
		this.leaveMessageTime = leaveMessageTime;
		this.comment = comment;
	}
	
	
	
}
